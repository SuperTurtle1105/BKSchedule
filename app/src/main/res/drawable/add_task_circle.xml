<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape xmlns:android="http://schemas.android.com/apk/res/android"
            android:shape="oval">
            <solid android:color="@color/white" />
            <size
                android:width="60dp"
                android:height="60dp" />
        </shape>
    </item>

    <item
        android:bottom="15dp"
        android:left="27dp"
        android:right="27dp"
        android:top="15dp">
        <shape>
            <stroke
                android:width="0dp"
                android:color="#ff207d94" />
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />
            <corners android:radius="3.5dp" />
            <solid android:color="@color/black" />
        </shape>
    </item>

    <item
        android:bottom="27dp"
        android:left="15dp"
        android:right="15dp"
        android:top="27dp">
        <shape>
            <stroke
                android:width="0dp"
                android:color="#ff207d94" />
            <padding
                android:bottom="0dp"
                android:left="0dp"
                android:right="0dp"
                android:top="0dp" />
            <corners android:radius="3.5dp" />
            <solid android:color="@color/black" />
        </shape>
    </item>
</layer-list>